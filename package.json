{
  "name": "mern-feedly",
  "version": "1.0.0",
  "description": "Interest based news feed Shows the complete article",
  "main": "index.js",
  "scripts": {
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev:debug": "concurrently \"npm run server:debug\" \"npm run frontend\"",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "start": "node app.js",
    "server": "nodemon app.js",
    "server:debug": "nodemon --inspect app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbirondo/MERN-feedly.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jbirondo/MERN-feedly/issues"
  },
  "homepage": "https://github.com/jbirondo/MERN-feedly#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "http": "0.0.0",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.9.14",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "redux": "^4.0.5",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.4"
  }
}
